<footer class="container bg-light py-5" id="here">
    <div class="container text-center">
        <h2 class="text-dark" style="font-family: 'Roboto Slab'; font-size: 40px;">
            Subscribe now and get 20% off
        </h2>
        <form class="mt-4">
            <input type="text" class="form-control d-inline" placeholder="Enter your email" style="max-width: 313px;" />
            <button class="btn rounded-pill btn-primary ms-2" style="padding: 10px 30px;">
                Subscribe
            </button>
        </form>
    </div>
    
  <div class="d-flex mt-4">
        {{#each this}}
        <div class="col-2 d-flex flex-column  pt-2 text-start" style="min-width:195px;">
            <strong class="mb-4">{{title}}</strong>
            {{#each links}}
            <a href="{{url}}" class="text-black text-decoration-none mb-3">{{name}}</a>
            {{/each}}
        </div>
        {{/each}} 
    </div>


</footer>